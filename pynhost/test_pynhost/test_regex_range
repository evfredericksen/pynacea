import unittest
import string
import sys
import os
import tempfile
import zipfile
import shutil
import copy
from pynhost import grammarbase
from pynhost import ruleparser
from pynhost.regex_range import regex_for_range

class TestRegexConvert(unittest.TestCase):

    @classmethod
    def setUpClass(cls):
        cls.num_text = ''

    def test_surround_previous_word1(self):
        reg = regex_for_range(12, 34)
        self.assertEqual(reg, 'hello (world )')


def num(n):
    return r'((?P<num{}>-?\d+(\.d+)?) |one |zero )'.format(n)

if __name__ == '__main__':
    unittest.main()
